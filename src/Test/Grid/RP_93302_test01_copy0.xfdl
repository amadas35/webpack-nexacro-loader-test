    <Script type="xscript5.1"><![CDATA[
this.svr_param1 = "";
this.startTime = null;
this.endTime = null;
this.Button00_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{

	var _use_firefirstcount = this.CheckBox03.value;	
	nexacro._use_firefirstcount = _use_firefirstcount;

	var _use_progressdata = this.CheckBox05.value;	
	nexacro._use_progress_data = _use_progressdata;
	
		
	this.Dataset00.set_progressload(this.CheckBox06.value);
	this.Dataset01.set_progressload(this.CheckBox07.value);
	this.Dataset02.set_progressload(this.CheckBox06.value);
	this.Dataset03.set_progressload(this.CheckBox07.value);
	
	var no_bind = this.CheckBox04.value;
	if (no_bind)
	{
		this.Grid00.set_binddataset("");
		this.Grid01.set_binddataset("");
	}
	else
	{
		this.Grid00.set_binddataset("Dataset00");
		this.Grid01.set_binddataset("Dataset01");
	}
		this.Dataset00.clear();
		this.Dataset01.clear();
		
	var svc_url = this.edt_url.value;
	var async = this.CheckBox00.value;
	var compress = this.CheckBox01.value;

	var firstrowcount1 = this.Edit01.value;
	var firstrowcount2 = this.Edit02.value;
	
	this.Dataset00.set_firefirstcount(firstrowcount1);
	this.Dataset01.set_firefirstcount(firstrowcount2);
	
	var strParams = "type=" + this.Radio00.value + " ";
		strParams += "rowcount=" + this.medt_rowcount.value + " ";
		strParams += "firstrowcount1=" + firstrowcount1 + " ";
		strParams += "firstrowcount2=" + firstrowcount2 + " ";
		strParams += "compress=" + this.CheckBox01.value + " ";
		strParams += "parameters=" + this.CheckBox02.value + " ";
		strParams += "receiveDsCount=" + this.Radio01.value + " ";;
		strParams += "errorcode=" + this.edt_errorcode.value;
		//strParams += "receiveDsCount=10000";
		
	var out_ds_str = "";

	if (this.Radio01.value == "1")
		out_ds_str = "Dataset00=outds00";
	else if (this.Radio01.value == "4")
		out_ds_str = "Dataset00=outds00 Dataset01=outds01 Dataset02=outds02 Dataset03=outds03";
			
	this.svr_param1 = "";
	nexacro.setEnvironmentVariable("svr_param2","");

    var date = new Date();
    this.startTime = date.getTime();    
    this.ds_in.clearData();
    this.ds_in.copyRow(this.ds_in.addRow(),this.ds_inData,this.ds_inData.rowposition);
	
	trace(strParams);
	//this.Div00.set_url("Base::form_dummy.xfdl");
	//this.Div01.set_url("Base::form_dummy.xfdl");
	trace("debug 1");
	this.transaction("test",svc_url, "ds_in=ds_in",out_ds_str,strParams,"fncallback",async);
	//this.transaction("test2",svc_url, "ds_in=ds_in",out_ds_str,strParams,"fncallback",async);
	trace("debug 2");
}

this.fncallback = function(sid,errcd,errmsg)
{
	trace("fncallback", sid , this.Dataset00.rowcount, this.Dataset01.rowcount, this.Dataset02.rowcount, this.Dataset03.rowcount);
	var date = new Date();
    this.endTime = date.getTime();  
    var p_time = (this.endTime - this.startTime)/1000;
   
	this.TextArea00.set_value(p_time +"\n"+(this.TextArea00.value ? this.TextArea00.value : ""));
	
	
	this.edt_param1.set_value(this.svr_param1);
	var env = nexacro.getEnvironment();	
	this.edt_param2.set_value(nexacro.getEnvironmentVariable("svr_param2"));
}

