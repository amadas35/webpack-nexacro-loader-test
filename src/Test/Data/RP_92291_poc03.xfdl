    <Script type="xscript5.1"><![CDATA[this.renderFlag = false;
this.startTime;

this.Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var firstRowCount = 1000;
	this.renderFlag = false;
	this.Static00.set_text("");
	this.Static01.set_text("");
	
	this.Grid00.vscrollbar.set_pos(0);
	this.dsList.clearData();
	//this.Grid00.set_binddataset('');
	//this.Grid00.set_enableredraw(false);
	
	//this.setTimer(100, 500);
		
	this.startTime = new Date();
	
	this._requery = false;
	nexacro._use_progress_data = this.ChkProgressLoad.isChecked();
	nexacro._transfer_data_mode = (this.ChkArrayBuffer.isChecked() ? 1 : 0);
	
	var reqUrl, inParam = "", row_count = this.dsSearch.getColumn(0, "searchRowCount")|0;

	switch (row_count)
	{
	  case 10000:
		reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-10K.ssv";
		break;
	  /*case 100000:
		reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-100K.ssv";
		break;
	  case 200000:
		reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-200K.ssv";
		break;
	  case 300000:
		reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-300K.ssv";
		break;
	  case 500000:
		reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-500K.ssv";
		break;
	  case 1000000:
		reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-1M.ssv";
		break;*/
	  default:
		reqUrl = "svc92291::coSysDownloadLogPoc/JdbcLargeDataList.do";
		inParam = "input=dsSearch";
		break;
	}
	
	trace("[use_progress_data]:" + nexacro._use_progress_data);
	trace("[transfer_data_mode]:" + nexacro._transfer_data_mode);
	trace("Request URL:" + reqUrl + " ("+ inParam + ")");
	this.transaction("search", reqUrl, inParam, "dsList=output dsFirstRowStatus=FirstRowStatus", "firstRowCount="+firstRowCount, "fnCallback", true, 2);
	
	//this.transaction("search", "svcPoc::coSysDownloadLogPoc/JdbcLargeDataList.do", "input=dsSearch", "dsList=output dsFirstRowStatus=FirstRowStatus", "firstRowCount="+firstRowCount, "fnCallback", true, 2);
	//this.transaction("search", "svcPoc::coSysDownloadLogPoc/JdbcLargeDataList.do", "input=dsSearch", "dsList=output dsFirstRowStatus=FirstRowStatus", "firstRowCount="+firstRowCount, "fnCallback", true, 2);
};

this.fnCallback = function(svcID, errorCode, errorMsg) {
	// 에러 시 화면 처리 내역
	if (errorCode != 0) {
		trace("[" + errorCode + "] " + errorMsg);
		alert("[" + errorCode + "] " + errorMsg);
		return;
	}
	
	switch (svcID) {
	case "search":
		var endTime = new Date();
		var elapsedTime = nexacro.round((endTime - this.startTime)/1000, 3);
		this.Static01.set_text("Total Time: "+elapsedTime+" sec");
		
		var strErrorCode = this.dsFirstRowStatus.getColumn(0, "ErrorCode"),
			strErrorMsg = this.dsFirstRowStatus.getColumn(0, "ErrorMsg");
		
		// 분할 전송 시 에러체크는 별도 데이터셋으로
		if (strErrorCode != 0) {
			trace("[" + strErrorCode + "] " + strErrorMsg);
			alert("[" + strErrorCode + "] " + strErrorMsg);
			return;
		}
		this.Grid00.vscrollbar.set_pos(0);
		//trace(this.dsList.saveXML());
		//this.Grid00.createFormat();
		//trace(this.Grid00.getCurFormatString());
		break;
	
	default:
	}
};

this.dsList_onload = function(obj:nexacro.NormalDataset,e:nexacro.DSLoadEventInfo)
{
	//trace('dsList_onload: progressload=' + e.progressload + ', reason=' + e.reason);
	
	if (this.renderFlag == false) {
		this.renderFlag = true;
		var renderTime = new Date();
		var elapsedTime = nexacro.round((renderTime - this.startTime)/1000, 3);
		this.Static00.set_text("First Rendering Time: "+elapsedTime+" sec");
		this.setWaitCursor(false, true);
	}
	
	if (this.Grid00.binddataset == '' && e.reason == 0)
		this.Grid00.set_binddataset('dsList');
		
	if (!this.Grid00.enableredraw && e.reason == 0)
		this.Grid00.set_enableredraw(true);
};


this.form_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.ChkProgressLoad.set_value(nexacro._use_progress_data);
	this.ChkArrayBuffer.set_value(nexacro._transfer_data_mode == 1);
};

this.form_ontimer = function(obj:nexacro.Form,e:nexacro.TimerEventInfo)
{
	if (e.timerid == 100)
	{
		this.killTimer(e.timerid);
		
		this.startTime = new Date();
		
		var firstRowCount = 1000;
		nexacro._use_progress_data = !!(this.ChkProgressLoad.value);
		nexacro._transfer_data_mode = (this.ChkArrayBuffer.value ? 1 : 0);
		
		var reqUrl, inParam = "", row_count = this.dsSearch.getColumn(0, "searchRowCount")|0;

		switch (row_count)
		{
		  case 10000:
			reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-10K.ssv";
			break;
		  case 100000:
			reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-100K.ssv";
			break;
		  case 200000:
			reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-200K.ssv";
			break;
		  case 300000:
			reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-300K.ssv";
			break;
		  case 500000:
			reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-500K.ssv";
			break;
		  case 1000000:
			reqUrl = "Resources::data-files/RP_92291_kopec-ui-poc-1M.ssv";
			break;
		  default:
			reqUrl = "svc92291::coSysDownloadLogPoc/JdbcLargeDataList.do";
			inParam = "input=dsSearch";
			break;
		}
		
		trace("[use_progress_data]:" + nexacro._use_progress_data);
		trace("[transfer_data_mode]:" + nexacro._transfer_data_mode);
		trace("Request URL:" + reqUrl + " ("+ inParam + ")");
		this.transaction("search", reqUrl, inParam, "dsList=output dsFirstRowStatus=FirstRowStatus", "firstRowCount="+firstRowCount, "fnCallback", true, 2);
		
	}
};

